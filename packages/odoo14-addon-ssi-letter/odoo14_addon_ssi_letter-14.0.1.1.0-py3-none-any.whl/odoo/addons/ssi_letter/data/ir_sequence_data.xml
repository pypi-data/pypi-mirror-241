<odoo>
<data noupdate="1">
<record id="outgoing_letter_sequence" model="ir.sequence">
    <field name="name">Outgoing Letter</field>
    <field name="code">outgoing_letter</field>
    <field name="prefix">OLE/%(range_year)s/</field>
    <field eval="1" name="number_next" />
    <field eval="1" name="number_increment" />
    <field name="padding" eval="6" />
    <field name="use_date_range" eval="1" />
    <field eval="False" name="company_id" />
</record>

<record id="incoming_letter_sequence" model="ir.sequence">
    <field name="name">Incoming Letter</field>
    <field name="code">incoming_letter</field>
    <field name="prefix">ILE/%(range_year)s/</field>
    <field eval="1" name="number_next" />
    <field eval="1" name="number_increment" />
    <field name="padding" eval="6" />
    <field name="use_date_range" eval="1" />
    <field eval="False" name="company_id" />
</record>

</data>
</odoo>
